<?xml version="1.0" encoding="UTF-8"?>
<project>
  <modelVersion>4.0.0</modelVersion>
  <artifactId>multisim-core-server</artifactId>
  <packaging>jar</packaging>
  <version>1.0.0-SNAPSHOT</version>
    <parent>
        <groupId>org.mili.multisim</groupId>
        <artifactId>multisim-core</artifactId>
        <version>1.0.0-SNAPSHOT</version>
    </parent>
  <dependencies>

      <dependency>
          <groupId>org.mili.multisim</groupId>
          <artifactId>multisim-plugin</artifactId>
          <version>${version}</version>
      </dependency>

      <dependency>
          <groupId>org.mili.multisim</groupId>
          <artifactId>multisim-util</artifactId>
          <version>${version}</version>
      </dependency>

      <dependency>
          <groupId>org.mili.multisim</groupId>
          <artifactId>multisim-core-client</artifactId>
          <version>${version}</version>
      </dependency>

      <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.11</version>
      <scope>test</scope>
    </dependency>
      <dependency>
          <groupId>javax.servlet</groupId>
          <artifactId>servlet-api</artifactId>
          <version>2.5</version>
      </dependency>
      <dependency>
          <groupId>org.apache.httpcomponents</groupId>
          <artifactId>httpclient</artifactId>
